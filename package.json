{
  "name": "parasnath-trekking",
  "version": "1.0.0",
  "description": "Fullstack application for Parasnath trekking",
  "scripts": {
    "install-client": "cd client && npm install",
    "install-server": "cd server && npm install",
    "build-client": "cd client && npm run build",
    "start-server": "cd server && node server.js",
    "dev": "concurrently \"cd client && npm start\" \"cd server && npm run dev\"",
    "install-all": "npm run install-client && npm run install-server",
    "build": "npm run build-client",
    "start": "npm run start-server",
    "prod": "NODE_ENV=production npm run start",
    "test": "cd client && npm test && cd ../server && npm test"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [],
  "author": "Your Name",
  "license": "ISC"
}